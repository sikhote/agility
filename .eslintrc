{
  "extends": ["airbnb", "prettier"],
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "rules": {
    "arrow-parens": 0,
    "consistent-return": 0,
    "global-require": 0,
    "import/no-dynamic-require": 0,
    "import/extensions": [2, { ignore: ["next", "react"] }],
    "import/no-unresolved": [2, { ignore: ["next", "react"] }],
    "import/prefer-default-export": 0,
    "max-len": [2, 80],
    "no-nested-ternary": 0,
    "no-shadow": 0,
    "no-underscore-dangle": 0,
    "radix": 0,
    "react/forbid-prop-types": 0,
    "react/jsx-filename-extension": 0,
    "react/no-danger": 0,
    "jsx-a11y/anchor-is-valid": 0
  }
}
