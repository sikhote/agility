    "babel-eslint": "8.0.1",
    "eslint": "4.19.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-prettier": "2.8.0",
    "eslint-plugin-import": "2.11.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "express": "^4.16.4",
    "express-history-api-fallback": "^2.2.1",
    "prettier": "1.14.2",
